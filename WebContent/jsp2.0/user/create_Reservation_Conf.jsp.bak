<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ include file="/jsp/common/taglibs.jsp"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html lang="zh-CN">
<head>
<meta charset="utf-8" />
<title>${siteBase.siteName} - ${LANG['website.common.index.title.base.1'] }</title>
<!--[if lt IE 9]><meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" /><![endif]-->
<!--[if lt IE 9]><script src="/assets/js/lib/html5.js"></script><![endif]-->
<!--[if lt IE 7]><script src="/assets/js/lib/belatedpng.js"></script><![endif]-->
<link type="image/x-icon" rel="shortcut icon" href="/assets/favicon.ico" />
<link rel="stylesheet" type="text/css" href="/assets/css/module/jquery-ui-1.9.2.custom.css"/>
<link type="text/css" rel="stylesheet" href="/assets/css/base.css" />
<link type="text/css" rel="stylesheet" href="/assets/css/apps/dialog.css" />
<link type="text/css" rel="stylesheet" href="/assets/css/apps/biz.validate.css" />
<cc:confList var="WEEK_DAYS"/>
<cc:confList var="CONF_VIDEO_TYPE_FLUENCY"/>
<cc:confList var="CONF_VIDEO_TYPE_DISTINCT"/>
<cc:confList var="CONF_VIDEO_TYPE_WEBBAND"/>
<cc:confList var="CONF_VIDEO_TYPE_BEST"/>
<cc:confList var="CONF_VIDEO_TYPE_FLUENCY_CODE"/>
<cc:confList var="CONF_VIDEO_TYPE_DISTINCT_CODE"/>
<cc:confList var="CONF_VIDEO_TYPE_WEBBAND_CODE"/>
<cc:confList var="CONF_VIDEO_TYPE_BEST_CODE"/>
<fmt:formatDate var="beijingDate" value="${defaultDate}" type="date" pattern="yyyy-MM-dd HH:mm:ss"/>
</head>
<body>
	<div class="meeting-dialog">
		<!-- 预约会议第一步 -->
		<div class="step1-panel valid-group">
			<div class="step1-header">
				<h3 class="title">
				<i class="icon icon-meeting">&nbsp;</i><span class="confOperate">${LANG['website.user.create.reser.page.divtitle'] }</span><span class="step">
				<ul class="numbers">
					<li class="active">1</li><li>2</li><li>3</li><li>4</li>
				</ul>
				<ul class="desc">
					<li>${LANG['website.user.create.reser.page.step.confinfo']}</li>
					<li>${LANG['website.user.create.reser.page.step.dateinfo']}</li>
					<li>${LANG['website.user.create.reser.page.step.paraminfo']}</li>
					<li>${LANG['website.user.create.reser.page.step.finish']}</li>
				</ul></span>
				</h3>
				<a href="javascript:closeDialog();" title="${LANG['website.user.create.reser.page.divclose']}" class="close"></a>
			</div>
			<form id="scheduleForm1" method="post">
			<div class="step-content">
				<h4 class="title">${LANG['website.user.create.reser.page.step.one.title']}</h4>
				<div class="form-panel">
						<div class="form-item">
							<label class="title">${LANG['website.user.create.reser.page.confname']}${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<input id="confName" name="confName" type="text" class="input-text" placeholder="${LANG['website.user.create.reser.page.confname']}" /> 
								<span class="required"></span> 
<!-- 								<span class="tips">1-32个任意字符</span> -->
								<span class="form-tip"><span id="confNameTip"></span></span>
							</div>
						</div>
						<div class="form-item">
							<label class="title">${LANG['website.user.create.reser.page.hostpassword']}${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<input id="hostKey" name="hostKey" type="text" class="input-text" placeholder="主持人密码" />
								<span class="required"></span> 
<!-- 									<span class="tips">6-12个数字 -->
									<span class="form-tip"><span id="hostKeyTip"></span></span>
								</span>
							</div>
						</div>
						<div class="form-item voice-widget">
							<label class="title">语音模式${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<input type="radio" name="voice" id="voiceNet" /> 
								<label for="voiceNet">网络语音</label> 
								<input type="radio" name="voice" id="netAndPhone" checked="checked"/> 
								<label for="netAndPhone">网络语音+电话语音</label>
							</div>
						</div>
						<div class="form-item public-widget">
							<label class="title">公开会议${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<input class="allowPublic" type="radio" name="allowPublic" id="publicYes" value="1"/> 
								<label for="publicYes">是</label> 
								<input class="allowPublic" type="radio" name="allowPublic" id="publicNot" value="2" checked="checked"/> 
								<label for="publicNot">否</label>
							</div>
						</div>
						<div class="form-item publicPassSet-widget">
							<label class="title">设置会议密码${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<input class="publicPass" type="radio" name="publicPass" id="publicYesPass" value="1"/> 
								<label for="publicYesPass">是</label> 
								<input class="publicPass" type="radio" name="publicPass" id="publicNotPass" value="2" checked="checked"/> 
								<label for="publicNotPass">否</label>
							</div>
						</div>
						<div class="form-item publicPass-widget confPassTR">
							<label class="title">会议密码${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<input id="confPass" name="confPass" type="password" class="input-text" placeholder="会议密码" />
								<span class="required"></span> 
<!--								<span class="tips">6-12个数字</span>-->
								<span class="form-tip"><span id="confPassTip"></span></span>
							</div>
						</div>
						<div class="form-item publicPass-widget confirmPassTR">
							<label class="title">确认会议密码${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<input id="confirmPass" name="confirmPass" type="password" class="input-text" placeholder="确认会议密码" />
								<span class="required"></span> 
<!--								<span class="tips">6-12个数字</span>-->
								<span class="form-tip"><span id="confirmPassTip"></span></span>
							</div>
						</div>
						<div class="form-item assign-widget confInviterTR">
							<label class="title">指定参会人${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<input type="radio" name="confInviterRadio" id="assignAll"  value="0" /> 
								<label for="assignAll">所有注册用户</label> 
								<input type="radio" name="confInviterRadio" id="assignSome" class="assign-some" checked="checked"  value="1" /> 
								<label for="assignSome">仅邀请用户</label>
								<span class="gray">(只有被邀请的注册用户可见)</span>
							</div>
						</div>
						<div class="form-item describe-widget">
							<label class="title">会议描述${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<textarea id="confDesc" class="input-area" placeholder="会议描述">${conf.confDesc}</textarea>
								<span class="count"><span id="nowCountSpan">0</span>/256</span>
							</div>
						</div>
				</div>
			</div>
			<div class="form-buttons">
				<input id="submit1" page-index="1" type="submit" class="button input-gray next-button" value="下一步">
				<a class="button anchor-cancel">取消</a>
			</div>
			</form>
		</div>
		
		<!-- 预约会议第二步 -->
		<div class="step2-panel valid-group" style="display: none;">
			<div class="step1-header">
				<h3 class="title">
				<i class="icon icon-meeting"></i><span class="confOperate">预约会议</span><span class="step">
				<ul class="numbers">
					<li class="active">1</li><li class="active">2</li><li>3</li><li>4</li>
				</ul>
				<ul class="desc">
					<li>会议信息</li>
					<li>时间信息</li>
					<li>参数信息</li>
					<li>完成</li>
				</ul></span>
				</h3>
				<a href="javascript:closeDialog();" title="关闭" class="close"></a>
			</div>
			<form id="scheduleForm2" method="post">
			<div class="step-content">
				<h4 class="title">${LANG['website.user.create.reser.page.step.two.title']}
					<span class="time" title="${beijingDate}">
						<c:if test="${conf == null}">
		                	<c:set var="timeZone" value="website.timezone.city.${currentUser.timeZoneId}"/>
		                	${LANG[timeZone]}时间&nbsp;
		                </c:if> 
		                <c:if test="${conf != null}">
		                	<c:set var="timeZoneDesc" value="website.timezone.city.${conf.timeZoneId}"/>
		                	${LANG[timeZoneDesc]}时间&nbsp;
		                </c:if>
						${beijingDate}
					</span>
				</h4>
				<div class="form-panel">
						<div class="form-item public-widget">
							<label class="title">重复周期${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<input class="repeat-cycle" type="radio" name="repeat" id="repeatYes" value="1"/>
								<label for="repeatYes">是</label>
								<input class="repeat-cycle" type="radio" name="repeat" id="repeatNot" checked="checked" value="2"/>
								<label for="repeatNot">否</label>
							</div>
						</div>
						<div class="form-item time-widget starttime-widget">
							<label class="title">开始时间${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<div>
									<input id="startTime" type="text" name="year" class="year input-text startDateDiv" readonly="readonly"/>
									<select id="startTimeH" class="hours">
										<c:forEach var="h" begin="00" end="23" step="1">
											<option value="${h}" >${h} 点</option>
										</c:forEach>
									</select>
									<select id="startTimeM" class="minute">
										<c:forEach var="m" begin="00" end="55" step="5">
											<option value="${m}">${m} 分</option>
										</c:forEach>
									</select>
									<span class="required"></span>
									<span class="form-tip"><span id="startTimeTip"></span></span>
								</div>
<!-- 								<div class="clearfix"></div> -->
								<div class="type-group duration-group" id="bytype">
									<input class="bytype" type="radio" name="bytype" id="overTime" value="1"/>
									<label for="overTime">按结束时间</label>
									<input class="bytype last-time" type="radio" name="bytype" id="lastTime" value="2" checked="checked"/>
									<label for="lastTime">按持续时间</label>
								</div>
							</div>
						</div>
						<div class="form-item time-widget endtime-widget">
							<label class="title">结束时间${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<input type="hidden" id="permanentConf_id" value="${conf.permanentConf}"/>
								<input id="endTime" type="text" name="year" class="year input-text" value="" readonly="readonly"/>
								<select id="endTimeH" class="hours">
									<c:forEach var="h" begin="00" end="23" step="1">
										<option value="${h}" >${h} 点</option>
									</c:forEach>
								</select>
								<select id="endTimeM" class="minute">
									<c:forEach var="m" begin="00" end="55" step="5">
										<option value="${m}">${m} 分</option>
									</c:forEach>
								</select>
								<span class="required"></span>
								<span class="form-tip"><span id="endTimeTip"></span></span>
							</div>
						</div>
						<div class="form-item duration-widget">
							<label class="title">持续时间${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<select id="durationH" class="hours">
									<c:forEach var="h" begin="00" end="23" step="1">
										<option value="${h}" <c:if test="${h==1}">selected="selected"</c:if>>${h}
											<c:if test="${h==1 || h==0}">
											小时
											</c:if>
											<c:if test="${h!=1 && h!=0}">
											小时
											</c:if> 
										</option>
									</c:forEach>
								</select>
								<select id="durationM" class="minute">
									<c:forEach var="m" begin="00" end="55" step="5">
										<option value="${m}">${m} 
											<c:if test="${m==0}">
											分钟
											</c:if>
											<c:if test="${m!=0}">
											分钟
											</c:if>
										</option>
									</c:forEach>
								</select>
								<span class="required"></span>
								<span class="form-tip"><span id="durationTip"></span></span>
							</div>
						</div>
						<div class="repeat-widget cycleTypeTR" style="display: none;">
							<div class="form-item">
								<p class="legend">定期模式</p>
								<div class="groups tabview clearfix">
									<ul class="ontime tabs">
										<li class="tab" data-tab="day"><label><input class="cycleType" type="radio" name="ontime" value="1"/><span>按天</span></label></li>
										<li class="tab active" data-tab="week"><label><input class="cycleType" type="radio" name="ontime" value="2" checked="checked" /><span>按周</span></label></li>
										<li class="tab" data-tab="month"><label><input class="cycleType" type="radio" name="ontime" value="3"/><span>按月</span></label></li>
									</ul>
									<div class="timemode views">
										<div class="view" data-view="day">
											<p>填写天数${LANG['website.common.symbol.colon']}</p>
<!--											<div class="bydate error-item">-->
											<div class="bydate">
												每 <input id="cycleDayFlag" type="text" class="input-text days" name="day" value="1"/> 天
<!-- 												<span class="error"><i class="icon"></i>请输入正确的时间间隔</span> -->
												<div class="cycleDayFlag-tip">
													<span class="form-tip"><span id="cycleDayFlagTip"></span></span>
												</div>
											</div>
										</div>
										<div class="view active" data-view="week">
											<p>选择星期${LANG['website.common.symbol.colon']}</p>
											<div class="dayofweek">
												<label><input type="checkbox" name="weekday" value="1"/><span>周日</span></label>
												<label><input type="checkbox" name="weekday" value="2" /><span>周一</span></label>
												<label><input type="checkbox" name="weekday" value="3" /><span>周二</span></label>
												<label><input type="checkbox" name="weekday" value="4" /><span>周三</span></label>
												<label><input type="checkbox" name="weekday" value="5" /><span>周四</span></label>
												<label><input type="checkbox" name="weekday" value="6" /><span>周五</span></label>
												<label><input type="checkbox" name="weekday" value="7" /><span>周六</span></label>
											</div>
											<div class="weekday-tip">
												<span class="form-tip"><span id="weekdayTip"></span></span>
											</div>
										</div>
										<div class="view" data-view="month">
											<p>选择日期${LANG['website.common.symbol.colon']}</p>
											<div class="bydate eachmonth-tip">
												<input type="radio" name="month-type" value="1"/>
												每月第 <input id="eachMonthDay" type="text" class="input-text days" name="day" value="1"/> 天
<!-- 												<span class="error"><i class="icon"></i>请输入正确的时间间隔</span> -->
												<span class="form-tip"><span id="eachMonthDayTip"></span></span>
											</div>
											<div class="byweek">
												<input type="radio" name="month-type" value="2" checked="checked"/>
												每月第
												<select id="weekFlag">
													<option value="1">1</option>
													<option value="2">2</option>
													<option value="3">3</option>
													<option value="4">4</option>
													<option value="5">5</option>
												</select>
												个
												<select id="weekDay">
													<c:forEach var="eachDay" items="${WEEK_DAYS}">
														<c:set var="week_day_lang" value="conf.cycle.weekly.${eachDay}"/>
														<option value="${eachDay}">${LANG[week_day_lang]}</option>
													</c:forEach>
												</select>
											</div>
										</div>
									</div>
								</div>
							</div>
							<div class="form-item repeat-item">
								<p class="legend">重复范围</p>
								<div class="groups clearfix">
									<ul class="ontime">
										<li>
											<input class="repeat-scope" id="endLess" type="radio" name="repeatScope" value="1"/>
											<label for="endLess"><span>无结束日期</span></label>
										</li>
										<li class="endRepeat-tip">
											<input class="repeat-scope" id="endRepeat" type="radio" name="repeatScope" value="2"/>
											<label for="endRepeat"><span class="equal-width">重复</span></label><input type="text" id="repeatCount" class="input-text times" value="30"/>
											<label for="endRepeat" style="margin-left: 60px;">次后结束</label>
											<span class="form-tip"><span id="repeatCountTip"></span></span>
										</li>
										<li class="endRepeat-foreEver-tip">
											<input class="repeat-scope" id="endOneDate" type="radio" name="repeatScope" value="3" checked="checked"/>
											<label for="endOneDate" style=""><span class="equal-width">结束日期</span></label>
											<span class="end-time-forever">
												<input id="endTimeForEver" type="text" class="input-text date" value="" readonly="readonly"/>
											</span>
											<span class="form-tip"><span id="endTimeForEverTip"></span></span>
										</li>
									</ul>
								</div>
							</div>
						</div>
					
				</div>
			</div>
			<div class="form-buttons">
				<input page-index="0" type="button" class="button input-gray prev-button" value="上一步">
				<input id="submit2" page-index="2" type="submit" class="button input-gray next-button" value="下一步">
				<a class="button anchor-cancel">取消</a>
			</div>
			</form>	
		</div>
		
		<!-- 预约会议第三步 -->
		<div class="step3-panel valid-group" style="display: none;">
			<div class="step1-header">
				<h3 class="title">
				<i class="icon icon-meeting"></i><span class="confOperate">预约会议</span><span class="step">
				<ul class="numbers">
					<li class="active">1</li><li class="active">2</li><li class="active">3</li><li>4</li>
				</ul>
				<ul class="desc">
					<li>会议信息</li>
					<li>时间信息</li>
					<li>参数信息</li>
					<li>完成</li>
				</ul></span>
				</h3>
				<a href="javascript:closeDialog();" title="关闭" class="close"></a>
			</div>
			<form  id="scheduleForm3" method="post" >
			<div class="step-content">
				<h4 class="title">${LANG['website.user.create.reser.page.step.three.title']}</h4>
				<div class="form-panel">
						<div class="form-item">
							<label class="title">会议类型${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<span id="videoFunc">
									<input type="checkbox" name="meetType" id="meetTypeVideo" value="2" checked="checked"/>
									<label for="meetTypeVideo">视频</label>
								</span>
							</div>
						</div>
						<div id="allowCallTr" class="form-item" style="display: none;">
							<label class="title">启动外呼${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<input type="radio" name="allowCall" id="compereMode" value="1" checked="checked" />
								<label for="compereMode">开启</label>
								<input type="radio" name="allowCall" id="freeMode" value="0" />
								<label for="freeMode">关闭</label>
							</div>
						</div>
						<div class="form-item public-widget">
							<label class="title">会议模式${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<input type="radio" name="meetMode" id="compereMode" value="1" checked="checked" />
								<label for="compereMode">主持人模式</label>
								<input type="radio" name="meetMode" id="freeMode" value="0" />
								<label for="freeMode">自由模式</label>
							</div>
						</div>
						<div class="form-item public-widget">
							<label class="title">开启麦克风${LANG['website.common.symbol.colon']}</label>
							<div class="widget">
								<input type="radio" name="mikeMode" id="enableMike" value="1"/>
								<label for="compereMode">是</label>
								<input type="radio" name="mikeMode" id="disableMike" value="0" checked="checked"/>
								<label for="freeMode">否</label>
							</div>
						</div>
						<div class="regular">
							<p class="legend" style="margin-bottom: 10px;"><a class="config">高级参数</a></p>
							<div class="config-details">
							<div class="form-item max-audio-widget">
								<label class="title">最大音频路数${LANG['website.common.symbol.colon']}</label>
								<div class="widget">
									<select id="maxAudio" class="short-select">
										<c:forEach var="h" begin="0" end="${audioNumber }" step="1">
											<option value="${h}">${h} 路</option>
										</c:forEach>
									</select>
								</div>
							</div>
							<div class="form-item max-video-widget">
								<label class="title">最大视频路数${LANG['website.common.symbol.colon']}</label>
								<div class="widget">
									<select id="maxVideo" class="short-select">
										<c:forEach var="h" begin="0" end="${videoNumber }" step="1">
							        		<c:if test="${h<7 || h==16}">
							        			<option value="${h}">${h} 路</option>
							        		</c:if>
										</c:forEach>
									</select>
								</div>
							</div>
							<div class="form-item public-widget">
								<label class="title">视频画质设置${LANG['website.common.symbol.colon']}</label>
								<div class="widget">
									<input id="lowMode" name="videoQuality" type="radio" value="${CONF_VIDEO_TYPE_WEBBAND }" <c:if test="${userEmpower.dpiNumber==CONF_VIDEO_TYPE_WEBBAND_CODE }"> checked="checked" </c:if> /><label for="lowMode">低</label>
						            <c:if test="${userEmpower.dpiNumber>=CONF_VIDEO_TYPE_FLUENCY_CODE }">
