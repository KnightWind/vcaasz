	<script type="text/javascript">
	var isOutCallFlag = "${isOutCallFlag}";
	var serverDate = new Date("${serverDate}");
	var confID = "${conf.id}";
	var confHwid = "${conf.confHwid}";
	var timeZone = "${conf.timeZone}";
	var cycleID = "${conf.cycleId}";
